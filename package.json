{
  "name": "@ons/prototype-kit",
  "version": "1.0.1",
  "type": "module",
  "main": "index.js",
  "license": "MIT",
  "homepage": "https://github.com/ONSdigital/prototype-kit",
  "author": {
    "name": "ONS Digital"
  },
  "scripts": {
    "lint-staged": "lint-staged",
    "dedupe-deps": "npx yarn-deduplicate yarn.lock && yarn"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-checkout": "yarn check"
    }
  },
  "lint-staged": {
    "*.scss": [
      "prettier --print-width 140 --single-quote --parser postcss --write",
      "git add"
    ],
    "*.js": [
      "prettier --print-width 140 --single-quote --parser babylon --write",
      "eslint",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "remark",
      "git add"
    ]
  },
  "dependencies": {
    "@rollup/plugin-node-resolve": "^13.1.3",
    "autoprefixer": "^9.4.7",
    "babel-eslint": "^10.0.1",
    "babelify": "^10.0.0",
    "browser-sync": "^2.27.7",
    "cssnano": "^4.1.8",
    "express": "^4.17.1",
    "front-matter": "^4.0.2",
    "glob": "^7.1.3",
    "gulp": "^4.0.2",
    "gulp-dart-sass": "^1.0.2",
    "gulp-if": "^3.0.0",
    "gulp-postcss": "^9.0.1",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-svgo": "^2.2.1",
    "lodash": "^4.17.13",
    "merge-stream": "^2.0.0",
    "normalize.css": "^8.0.1",
    "nunjucks": "^3.2.4",
    "postcss": "^8.4.31",
    "postcss-mq-optimize": "^1.1.0",
    "postcss-url": "^8.0.0",
    "querystring-helpers": "^1.0.2",
    "rollup": "^2.67.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "watchify": "^4.0.0"
  },
  "devDependencies": {
    "eslint": "^5.13.0",
    "eslint-cli": "^1.1.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "prettier": "^1.16.4",
    "pretty": "^2.0.0",
    "remark": "^10.0.1",
    "remark-cli": "^6.0.1",
    "remark-lint": "^6.0.4",
    "remark-preset-lint-recommended": "^3.0.2"
  }
}
